{
  "name": "tohuwabohu-roundcube",
  "version": "3.8.2",
  "author": "tohuwabohu",
  "license": "Apache-2.0",
  "summary": "Install and manage the Roundcube webmail application",
  "source": "https://github.com/tohuwabohu/puppet-roundcube.git",
  "project_page": "https://github.com/tohuwabohu/puppet-roundcube",
  "issues_url": "https://github.com/tohuwabohu/puppet-roundcube/issues",
  "tags": [
    "roundcube",
    "webmail"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "20.04",
        "22.04",
        "24.04"
      ]
    },
    {
      "operatingsystem": "FreeBSD"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.0.0 < 9.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "willdurand/composer",
      "version_requirement": "1.x"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.0.0 < 10.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.6.0 < 10.0.0"
    }
  ]
}
